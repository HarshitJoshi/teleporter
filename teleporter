#!/usr/bin/env bash
function tp() {
    result=$( node ~/.scripts/pathfinder.js "$@")
    if [[ $result == *"Locations stored in DB:"* ]]; then
        echo $result
    elif [[ $result == *"Error:"* ]]; then
        echo $result
    elif [ -d "$result" ]; then
        cd $result
    elif [[ $result == *"Teleporter - A Node.js script for quick directory 'cd's"* ]]; then
        echo $result
    else
    fi
}
